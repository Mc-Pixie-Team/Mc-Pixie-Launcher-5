{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Mc-Pixie-Launcher-5",
  "version": "0.1.0",
  "identifier": "com.pixie.mclauncher",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm nuxtdev",
    "beforeBuildCommand": "pnpm generate"
  },
  "app": {
    "windows": [
      {
        "title": "Mc-Pixie Launcher",
        "width": 1200,
        "height": 700,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
  
}
